
<h1>Team</h1>

<div class="team">
  <h3>Team Name: <%= @team.name %></h3>
  <h4>Team Motto: <%= @team.motto %> </h4>
</div>
<h2>Heros</h2>
<% @heros.each do |hero| %>
  <div class="heros">
    <p>Hero Name: <%= hero.name %></p>
    <p>Hero Power: <%= hero.power %></p>
    <p>Hero Biography: <%= hero.bio %>
  </div><br>
<% end %>
