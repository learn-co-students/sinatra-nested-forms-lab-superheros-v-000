

<h1> <%= @params["team"]["name"] %> </h1>

<h1> Team Motto: <%= @params["team"]["motto"] %> </h1>






<% @heroes.each do |hero| %>

  <div class="hero">
    <h1> Hero Name: <%= hero["name"] %> </h1>
    <p>  Hero Power: <%= hero["power"] %></p>
    <p>  Hero Biography: <%= hero["bio"] %></p>
  </div><br>
<% end %>
