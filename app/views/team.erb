<div class="team">
  <h3><%= @team.name %></h3><br>
  <h4>Team Motto: <%= @team.motto %></h4>
</div><br>

<% @members.each do |member| %>
  <div class="member">
    <h1>Hero Name: <%= member.name %></h1>
    <p>Hero Power: <%= member.power %></p><br>
    <p>Hero Biography: <%= member.bio %></p><br>
  </div><br>
<% end %>
