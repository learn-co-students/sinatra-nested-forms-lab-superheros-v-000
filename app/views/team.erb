<h1>Teams and Heroes</h1>

<div class="team">
<h2>Team Name: <%= @team.name %></h2>
<h2>Team Motto: <%= @team.motto %></h2>
</div>

<h1>Heroes</h1>
<% @heroes.each do |hero| %>
  <div class="hero">
    <p>Hero Name: <%= hero.name %></p>
    <p>Hero Power: <%= hero.power %></p>
    <p>Hero Biography: <%= hero.biography %></p>
  </div>
  <% end %>
