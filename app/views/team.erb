<h1><%= @team[:name] %></h1>
    <h2>Team Motto: <%= @team[:motto] %></h3><br>

  <% @members.each do |member| %>
      <div class="hero">
        <h3>Hero Name: <%= member[:name] %></h3>
        <p>Hero Power: <%= member[:power] %></p>
        <p>Hero Biography: <%= member[:bio] %></p>
      </div>
    <% end %>
