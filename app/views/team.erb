
<h1><%= @team.name%></h1>
  <div class="team">
  <h2>Motto: <%= @team.motto %></h2>

  </div><br>


<% =@members.each do |member| %>
  <div class="member">
    <h2>Hero Name: <%= member.name %></h2><br>
    <h4>Hero Power: <%= member.power %></h4><br>
    <h4>Hero Biography: <%= member.bio %></h4><br>

  </div><br>
<%=end %>

