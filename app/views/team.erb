


<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Super Hero Teams</title>
  </head>
  <body>
    <h1><%= @team.name %></h1>
    <h2>Team Motto: <%= @team.motto%></h2>

    <% @members.each do |member| %>
      <h2>Hero Name: <%= member.name %> </h2>
      <p>Hero Power: <%= member.power %></p>
      <p>Hero Biography: <%= member.bio %></p>
    <% end %>

  </body>
</html>
