<div class="team">
  <h1><%= @team.name %></h1>
  <h2>Team Motto: <%= @team.motto %></h2>
</div>

<% @members.each do |member| %>
<div class="member">
  <h2>Hero Name: <%= member.name %></h2><br>
  <h4>Hero Power: <%= member.power %></h4><br>
  <h4>Hero Biography: <%= member.bio %></h4>
</div><br>
<% end %>
