
 <h1>Superhero Team Name: <%= @team_name %></h1>
 <h2>Team Motto: <%= @team_motto %></h2>
<hr>
<% @members.each do |member| %>
  <h2>Hero Name: <%=member[:name] %></h2>
  <ul>
  <li><h3>Hero Power: <%=member[:power] %></h3></li>
  <li><h3>Hero Biography: <%=member[:bio] %></h3></li>
</ul>
<% end %>
