<head>
      <link rel="stylesheet" href="stylesheets/style.css"> 
</head>
<h2><p>Team Name: <%= @team.name %></p></h2>
<h2><p>Team Motto: <%= @team.motto %></p></h2>

<% @heros.each do |hero| %>
<h3>Hero #<%= @heros.index(hero) + 1 %></h3>
      <p>Hero Name: <%= hero.name %></p>
      <p>Hero power: <%= hero.power %></p>
      <p>Hero biography: <%= hero.biography %></p>
<% end %>
