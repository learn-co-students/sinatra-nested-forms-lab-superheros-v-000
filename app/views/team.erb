
<div class="team">
  <h1><%= @team.name %></h1><br>
  <h2>Team Motto: <%= @team.motto %></h2><br>
</div>

<div class="heroes">
  <% @heroes.each do | hero_data| %>

  <h3>Hero Name: <%= hero_data.name %></h3>
  <p>Hero Power: <%= hero_data.power %></p>
  <p>Hero Biography: <%= hero_data.biography %></p><br>
  <% end %>
</div>
