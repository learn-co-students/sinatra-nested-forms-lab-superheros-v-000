<h1>Create a Team and Heroes!</h1>
<h2>Wat</h2>
<form action="/teams" method="POST">

  <label for="POST-name">Team Name:</label>
  <input id="POST-name" type="text" name="team[name]">

  <label for="POST-motto">Team Motto:</label>
  <input id="POST-motto" type="text" name="team[motto]">

  <br/>

  <label for="POST-member1_name">Member 1 Name:</label>
  <input id="member1_name" type="text" name="team[members][][name]">

  <label for="POST-member1_power">Member 1 Power:</label>
  <input id="member1_power" type="text" name="team[members][][power]">

  <label for="POST-member1_bio">Member 1 Biography:</label>
  <input id="member1_bio" type="text" name="team[members][][bio]">
  <br/>

  <label for="POST-member2_name">Member 2 Name:</label>
  <input id="member2_name" type="text" name="team[members][][name]">

  <label for="POST-member2_power">Member 2 Power:</label>
  <input id="member2_power" type="text" name="team[members][][power]">

  <label for="POST-member2_bio">Member 2 Biography:</label>
  <input id="member2_bio" type="text" name="team[members][][bio]">

  <br/>

  <label for="POST-member3_name">Member 3 Name:</label>
  <input id="member3_name" type="text" name="team[members][][name]">

  <label for="POST-member3_power">Member 3 Power:</label>
  <input id="member3_power" type="text" name="team[members][][power]">

  <label for="POST-member3_bio">Member 3 Biography:</label>
  <input id="member3_bio" type="text" name="team[members][][bio]">

  <button type="submit" value="submit">Submit</button>
</form>
