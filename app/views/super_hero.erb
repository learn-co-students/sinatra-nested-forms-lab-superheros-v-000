<h1>Create a Team and Heroes!</h1>
<form method = "POST" action = "/teams">
  Team Name: <input type="text" name="team[name]">
  Team Motto: <input type="text" name="team[motto]">
  Member 1 Name: <input type="text" id="member1_name" name="team[members][][name]">
  Member 1 Power: <input type="text" id="member1_power" name="team[members][][power]">
  Member 1 Bio: <input type="text" id="member1_bio" name="team[members][][bio]">
  Member 2 Name: <input type="text" id="member2_name" name="team[members][][name]">
  Member 2 Power: <input type="text" id="member2_power" name="team[members][][power]">
  Member 2 Bio: <input type="text" id="member2_bio" name="team[members][][bio]">
  Member 3 Name: <input type="text" id="member3_name" name="team[members][][name]">
  Member 3 Power: <input type="text" id="member3_power" name="team[members][][power]">
  Member 3 Bio: <input type="text" id="member3_bio" name="team[members][][bio]">
  <input type="submit">
</form>
