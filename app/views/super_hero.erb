<h2>Create a Team and Heroes!</h2>


<form action="/team" method="POST">
  <p>Team Name: <input type="text" id="team[name]" name="team[name]"></p>
  <p>Team Motto: <input type="text" id="team[motto]" name="team[motto]"></p>
  <p>Member 1 Name: <input type="text" id="member1_name" name="team[member][][name]"></p>
  <p>Member 1 Power: <input type="text" id="member1_power" name="team[member][][power]"></p>
  <p>Member 1 Bio: <input type="text" id="member1_bio" name="team[member][][bio]"></p>
  <p>Member 2 Name: <input type="text" id="member2_name" name="team[member][][name]"></p>
  <p>Member 2 Power: <input type="text" id="member2_power" name="team[member][][power]"></p>
  <p>Member 2 Bio: <input type="text" id="member2_bio" name="team[member][][bio]"></p>
  <p>Member 3 Name: <input type="text" id="member3_name" name="team[member][][name]"></p>
  <p>Member 3 Power <input type="text" id="member3_power" name="team[member][][power]"></p>
  <p>Member 3 Bio: <input type="text" id="member3_bio" name="team[member][][bio]"></p>
  <input type="submit" value="submit">
</form>
