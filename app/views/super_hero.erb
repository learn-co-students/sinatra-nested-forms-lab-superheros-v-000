<h1>Create a Team and Heroes!</h1>
<form method="POST" action="/team">

  <h1>Team Name: <%= @team.name %></h1>
  <h2>Team Motto: <%= @team.motto%></h1>

  <% @heroes.each do |hero| %>
   <h2>Hero Name: <%= hero.name %></h2>
   <p>Hero Power: <%= hero.power %></p>
   <p>Hero Biography: <%= hero.biography %></p>
 <% end %>
