<form action='/teams' method='POST'>
  <p>Team Name: <input type='text' name='team[name]'></p>
  <p>Team Motto: <input type='text' name='team[motto]'></p>
  <h3>Hero 1</h3>
  <p>Hero's name: <input type='text' name='member1_name'></p>
  <p>Hero's power: <input type='text' name='member1_power'></p>
  <p>Hero's biography: <input type='text' name='member1_bio'></p>

  <h3>Hero 2</h3>
  <p>Hero's name: <input type='text' name='member2_name'></p>
  <p>Hero's power: <input type='text' name='member2_power'></p>
  <p>Hero's biography: <input type='text' name='member2_bio'></p>

  <h3>Hero 3</h3>
  <p>Hero's name: <input type='text' name='member3_name'></p>
