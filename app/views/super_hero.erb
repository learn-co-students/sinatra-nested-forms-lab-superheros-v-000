<h1>Create a Team and Heroes!</h1>
<form method = "POST" action = "/teams">
  Team Name: <input type="text" name="team[name]"></br>
  Team Motto: <input type="text" name="team[motto]"></br>
  Member 1 Name: <input type="text" id="member1_name" name="team[members][][name]"></br>
  Member 1 Power: <input type="text" id="member1_power" name="team[members][][power]"></br>
  Member 1 Bio: <input type="text" id="member1_bio" name="team[members][][bio]"></br>
  Member 2 Name: <input type="text" id="member2_name" name="team[members][][name]"></br>
  Member 2 Power: <input type="text" id="member2_power" name="team[members][][power]"></br>
  Member 2 Bio: <input type="text" id="member2_bio" name="team[members][][bio]"></br>
  Member 3 Name: <input type="text" id="member3_name" name="team[members][][name]"></br>
  Member 3 Power: <input type="text" id="member3_power" name="team[members][][power]"></br>
  Member 3 Bio: <input type="text" id="member3_bio" name="team[members][][bio]"></br>
  <input type="submit" value='submit'>
</form>
