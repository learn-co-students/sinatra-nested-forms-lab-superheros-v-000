  <input type="text" id="member2_name" name="team[members][][name]" value=""><br>

  <label for="member2_power">Member 2 Power:</label>
  <input type="text" id="member2_power" name="team[members][][power]" value=""><br>

  <label for="member2_bio">Member 2 Bio:</label>
  <input type="text" id="member2_bio" name="team[members][][biography]" value=""><br>

  <h1>Hero 3</h1>
  <label for="member3_name">Member 3 Name:</label>
  <input type="text" id="member3_name" name="team[members][][name]" value=""><br>

  <label for="member3_power">Member 3 Power:</label>
  <input type="text" id="member3_power" name="team[members][][power]" value=""><br>

  <label for="member3_bio">Member 3 Bio:</label>
  <input type="text" id="member3_bio" name="team[members][][biography]" value=""><br>

  <input type="submit" id="submit">
