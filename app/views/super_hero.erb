<form  action="/teams" method="POST">
  <h1>Create a Team and Heroes!</h1>
  <h1>Team</h1>
  Team Name:<input type="text" name="team[name]">
  <br>
  Team Motto:<input type="text" name="team[motto]">
  <br>

  <h1>Hero 1</h1>
  Hero No. 1 Name:<input id="member1_name" type="text" name="team[heros][][name]">
  <br>
  Hero No. 1 Power:<input id="member1_power" type="text" name="team[heros][][power]">
  <br>
  Hero No. 1 Bio:<input id ="member1_bio" type="text" name="team[heros][][bio]">
  <br>

  <h1>Hero 2</h1>
  Hero No. 2 Name:<input id="member2_name" type="text" name="team[heros][][name]">
  <br>
  Hero No. 2 Power:<input id="member2_power" type="text" name="team[heros][][power]">
  <br>
  Hero No. 2 Bio:<input id="member2_bio" type="text" name="team[heros][][bio]">
  <br>

  <h1>Hero 3</h1>
  Hero No. 3 Name:<input id="member3_name" type="text" name="team[heros][][name]">
  <br>
  Hero No. 3 Power:<input id="member3_power" type="text" name="team[heros][][power]">
  <br>
  Hero No. 3 Bio:<input id ="member3_bio" type="text" name="team[heros][][bio]">
  <br>
  <button type="submit" id="submit">Submit</button>
  <!--team[heros][][attribute]]-->

</form>
