<!DOCTYPE html>
<head>
  <title>Superhero team <%= @team.name %></title>
</head>
<body>
  <h1><%= @team.name %></h1>
  <h3>Team Motto: <%= @team.motto %></h1>
  <% @members.each do |member| %>
    <h2>Hero Name: <%= member.name %></h2>
    <h3>Hero power: <%= member.power %></h3>
    <h3>Hero biography: <%= member.bio %></h3>
    <br>
  <% end %>
</body>
</html>

